{
  "name": "@pengleip/xmzx-utils",
  "version": "1.1.3",
  "description": "这个是一个js工具库",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "types": "index.d.ts",
  "private": false,
  "exports": {
    "require": "./dist/index.cjs.js",
    "import": "./dis/index.esm.js"
  },
  "scripts": {
    "test": "jest test/*",
    "lint:eslint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src --cache",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "build": "rollup -c --bundleConfigAsCjs",
    "commit": "git-cz",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh -r patch",
    "build:doc": "./scripts/deploy.sh",
    "commit:npm": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pengleimaxue/xmzx-utils"
  },
  "keywords": [
    "utils",
    "前端",
    "工具类"
  ],
  "author": "penglei",
  "files": [
    "dist"
  ],
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "rollup": "^3.2.5",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "standard-version": "^9.5.0",
    "typedoc": "^0.23.28",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@pengleip/xmzx-utils": "link:"
  }
}
